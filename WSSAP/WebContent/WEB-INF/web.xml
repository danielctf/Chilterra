<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
<servlet>
  <servlet-name>WSAutorizacion</servlet-name> 
  <servlet-class>cl.a2r.sap.servlet.WSAutorizacion</servlet-class> 
  </servlet>
<servlet>
  <servlet-name>WSMedicion</servlet-name> 
  <servlet-class>cl.a2r.sap.servlet.WSMedicion</servlet-class> 
  </servlet>
 <servlet-mapping>
  <servlet-name>WSAutorizacion</servlet-name> 
  <url-pattern>/WSAutorizacion</url-pattern> 
  </servlet-mapping>
 <servlet-mapping>
  <servlet-name>WSMedicion</servlet-name> 
  <url-pattern>/WSMedicion</url-pattern> 
  </servlet-mapping>
 <session-config>
  <session-timeout>30</session-timeout> 
  </session-config>
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file> 
  </welcome-file-list>
</web-app>